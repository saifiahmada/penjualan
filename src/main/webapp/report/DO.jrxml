	<queryString>
		<![CDATA[select h.NO_DO, h.NO_PO, po.TGL_PO, md.id, md.NAMA_DEALER,mi.NAMA, d.QTY, d.HARGA_NETTO, (d.QTY * d.HARGA_NETTO ) total  from hdr_do h
